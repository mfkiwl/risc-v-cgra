T_0 ;
    %wait E_0x600003bf5d40;
    %load/vec4 v0x600001cf41b0_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.0, 8;
    %load/vec4 v0x600001cf4120_0;
    %jmp/1 T_0.1, 8;
T_0.0 ; End of true expr.
    %load/vec4 v0x600001cf4090_0;
    %jmp/0 T_0.1, 8;
 ; End of false expr.
    %blend;
T_0.1;
    %store/vec4 v0x600001cf4000_0, 0, 32;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fbba2705b80;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "mux2_1testbench.vcd" {0 0 0};
    %end;
